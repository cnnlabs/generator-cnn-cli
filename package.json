{
  "name": "generator-cnn-cli",
  "version": "0.2.0",
  "description": "CNN CLI Package Generator",
  "main": "generators/app/index.js",
  "directories": {
    "test": "./test"
  },
  "scripts": {
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/generator-cnn-cli.git"
  },
  "homepage": "https://github.com/cnnlabs/generator-cnn-cli#readme",
  "engines": {
    "node": ">=5.6.0"
  },
  "dependencies": {
    "yeoman-generator": "1.0.0"
  },
  "devDependencies": {
    "changelog-maker": "2.2.0",
    "esdoc": "0.4.5",
    "eslint": "2.2.0",
    "jq-cli-wrapper": "0.3.0",
    "jsonlint": "1.6.2",
    "npm-check-updates": "2.5.8"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "yeoman-generator"
  ],
  "license": "MIT",
  "files": [
    "generators/app"
  ],
  "bugs": {
    "url": "https://github.com/cnnlabs/generator-cnn-cli/issues"
  }
}
